#include <stdio.h>

#define MAX 20

struct Process {
    int at, bt, wt;
};

int main() {

    struct Process p[MAX];
    int n, time = 0, done = 0;
    float avg = 0;

    scanf("%d", &n);

    for(int i=0;i<n;i++)
        scanf("%d%d", &p[i].at, &p[i].bt);

    int used[MAX] = {0};

    while(done < n) {

        int idx = -1, min = 9999;

        for(int i=0;i<n;i++) {

            if(p[i].at <= time && !used[i] && p[i].bt < min) {
                min = p[i].bt;
                idx = i;
            }
        }

        if(idx == -1) {
            time++;
            continue;
        }

        p[idx].wt = time - p[idx].at;
        time += p[idx].bt;

        used[idx] = 1;
        done++;

        avg += p[idx].wt;
    }

    printf("SJF = %.2f\n", avg/n);

    return 0;
}